/* packageA/pages/ShopCertification/ShopCertification.wxss */
@import '../selfGood/selfGood.wxss';

page {
  background: #fff;
}

.selfgood-add-head-six-img {
  margin-top: 32rpx;
  width: 686rpx;
  height: 360rpx;
  background: rgba(241, 242, 246, 1);
  opacity: 1;
  position: relative;
}

.selfgood-add-head-six-img image {
  width: 100%;
  height: 100%;
}

.selfgood-add-head-sixbox {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 36rpx;
  color: rgba(77, 77, 77, 1);
  opacity: 1;
  display: flex;
  flex-direction: column;
  padding: 30rpx 0;
}

.selfgood-add-head-sixbox .delImg {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  top: 0;
  right: 0;
}
