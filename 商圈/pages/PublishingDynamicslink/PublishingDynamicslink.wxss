/* pages/PublishingDynamicslink/PublishingDynamicslink.wxss */
page{
  background: #fff;
}
.publishDynamic-link-container{
  padding: 32rpx;
}
.publishDynamic-link{
  display: flex;
  flex-direction: column;
}
.publishDynamic-link text:nth-child(1){
  font-size:36rpx;
font-family:PingFang SC;
font-weight:400;
line-height:93rpx;
color:rgba(51,51,51,1);
}
.publishDynamic-link text:nth-child(2){
font-size:22rpx;
font-family:PingFang SC;
font-weight:400;
line-height:54rpx;
color:rgba(102,102,102,1);
}
.publishDynamic-link-input{
  width: 658rpx;
  border-bottom: 1px solid #999999;
  display: flex;
  justify-content: space-between;
  margin:93rpx auto 120rpx;
}
.publishDynamic-link-input input{
  width: 600rpx;
}
.publishDynamic-link-input image{
  height: 28rpx;
  width: 28rpx;
}
.placeholderclass{
  font-size:28rpx;
font-family:PingFang SC;
font-weight:400;
line-height:69px;
color:rgba(153,153,153,1);
}
.publishDynamic-link-container button{
width:200rpx;
height:98rpx;
background:rgba(215,0,15,1);
opacity:1;
border-radius:49px;
font-size:28rpx;
font-family:PingFang SC;
font-weight:400;
line-height:98rpx;
color:rgba(255,255,255,1)
}
.publishDynamic-link-container button::after{
  border: none;
}