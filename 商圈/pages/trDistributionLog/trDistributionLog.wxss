/* pages/trDistributionLog/trDistributionLog.wxss */
page{
  background: #fff;
}
.distributionlog-container{
  padding: 0 32rpx ;
}
.distributionlog-container-item{
  display: flex;
  flex-direction: column;
  padding: 35rpx 0;
    border-bottom: 1px solid #E6E6E6;
    font-family:PingFang SC;
font-weight:400;
line-height:36px;
}
.distributionlog-container-item-head,.distributionlog-container-item-bottom{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-right: 16rpx;
}
.distributionlog-container-item-head .distributionlog-container-item-head-name{
font-size:30rpx;
font-family:PingFang SC;
font-weight:400;
line-height:40px;
}
.distributionlog-container-item-head-tel {
  font-size:24rpx;
color:rgba(153,153,153,1);
}
.distributionlog-container-item-head-order,.distributionlog-container-item-bottom-time{
  font-size:24rpx;
  color:rgba(77,77,77,1);
}
.distributionlog-container-item-bottom-money{
  font-size:21rpx;
}
.distributionlog-container-item-bottom-money text:nth-child(1){
  color:#999999;
}
.distributionlog-container-item-bottom-money text:nth-child(2){
  color:#D7000F;
}