/* pages/settingcode/settingcode.wxss */
page{
  background: #fff;
}
.setting-codes{
display: flex;
flex-direction: column;
align-items: center;
padding-top: 310rpx;

}
.setting-codes-item{
  width: 400rpx;
  height:100rpx;
  background: #F1F2F6;
  line-height: 100rpx;
  text-align: center;
  color: #333333;
}
.setting-codes-btns{
  display: flex;
  align-items: center;
  width:300rpx;
  margin-top: 32rpx;
}
.setting-codes-btns button::after{
  border:1px solid #999999;
      border-radius:30px;
}
.setting-codes-btns button{
    background: #fff;
    color: #333333;
    font-size: 24rpx;
    height: 60rpx;
    width: 120rpx;
}
.code-info{
  font-size: 24rpx;
  color:#4D4D4D;
  margin-top: 195rpx;
}
.code-info-item{
  display: flex;
  flex-direction: column;
  margin-left: 38rpx;
  font-family:PingFang SC;
font-weight:400;
line-height:36rpx;
}